{
  "uploadDir": "tasks-appendices",
  "compression": { 
    "uploadDir": "compressed",
    "sharp_to_format_jpeg": {
      "type": 1, 
      "quality": 60,
      "filenameSuffix":"sharp_to_format_jpeg"
    },
    "sharp_to_format_png": {
      "type": 2, 
      "quality": 60,
      "filenameSuffix":"sharp_to_format_png"
    }
  },
  "resize": {
    "uploadDir": "resized",
    "sharp_resize": {
      "type": 5,
      "filenameSuffix":"sharp_resize",
      "height": 768,
      "width": 1024
    }
  },
  "archivisation": { 
    "enabledForImages": 0,
    "uploadDir": "archived",
    "type": "zlib_gzip",
    "zlib_gzip": {
      "type": 3,
      "filenameSuffix":"zlib_gzip",
      "fileExtToAppend": "gz"
    },
    "zlib_brotli": {
      "type": 4,
      "filenameSuffix":"zlib_brotli",
      "fileExtToAppend": "br"
    }
  }
}